unordered_map<int,int>mp;
        int result=0;
        for(int i=0;i<points.size();i++){
            for(int j=0;j<points.size();j++){
                
        int  d=pow(points[j][1]-points[i][1],2) + pow(points[j][0]-points[i][0],2);
                result+=2*mp[d];
                mp[d]++;
            }
            mp.clear();
        }
        return result;
